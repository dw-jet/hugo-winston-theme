{
  "baseURL": "/",
  "languageCode": "en-us",
  "theme": "hugo-winston-theme",
  "themesDir": "themes",
  "title": "Adventures in the Dark",
  "paginate": 3,
  "pygmentsCodeFences": true,
  "pygmentsCodefencesGuessSyntax": true,
  "pygmentsUseClasses": true,
  "module": {
    "hugoVersion": {
      "extended": true,
      "min": "0.55.0"
    }
  },
  "params": {
    "google_analytics_id": "",
    "twitter_handle": "@jeteitsworth",
    "showAuthorOnHomepage": true,
    "showAuthorOnPosts": false,
    "showIntroContentOnHomepage": true,
    "showPostsOnHomepage": true,
    "usePaginationOnHomepage": false,
    "limitPostsOnHomepage": 2,
    "sortPostsByDateOldestFirst": false,
    "addDot": true,
    "addFrame": true,
    "highlightColor": "#1FB600",
    "baseColor": "#ffffff",
    "baseOffsetColor": "#eaeaea",
    "headingColor": "#1c1b1d",
    "textColor": "#4e5157",
    "dotColor": "#1FB600",
    "enableGoogleFonts": true,
    "googleFontsUrl": "https://fonts.googleapis.com/css2?family=Poppins:wght@400;700&display=swap",
    "fontFamilyHeading": "Poppins",
    "fontFamilyParagraph": "Helvetica",
    "fontFamilyMonospace": "monospace"
  },
  "markup": {
    "goldmark": {
      "renderer": {
        "hardWraps": false,
        "unsafe": true,
        "xhtml": false
      }
    }
  },
  "menu": {
    "main": [
      {
        "identifier": "home",
        "name": "Home",
        "url": "/",
        "weight": 1
      },
      {
        "name": "Blog",
        "identifier": "blog",
        "weight": 2,
        "url": "/posts/"
      },
      {
        "name": "About",
        "identifier": "about",
        "weight": 3,
        "url": "/About/"
      },
      {
        "name": "Tags",
        "identifier": "tags",
        "weight": 4,
        "url": "/tags/"
      }
    ]
  }
}
